package Main.use_cases.viewfavourites;


public class FavouritesOutputData {
    private final List<String> favoriteRecipeNames;

    public FavouritesOutputData(List <String> favoriteRecipeNames) {
        this.favoriteRecipeNames = favoriteRecipeNames;
    }

    public List<String> ReturnFavoriteRecipeNames() {
        return favoriteRecipeNames;
    }
}
